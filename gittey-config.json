{
    "branchPrefix": {
        "separator": "",
        "validator": "",
        "prefixes": {}
    },
    "commitPrefix": {
        "separator": " ",
        "validator": "^.{1,45}$",
        "prefixes": {
            "feat:": "Feature work",
            "refactor:": "Refactoring",
            "test:": "Test",
            "fix:": "Bug fix",
            "chore:": "Chore",
            "docs:": "Documentation updated/added",
            "style:": "Code style"
        }
    },
    "verboseMode": false,
    "aliases": [
        {
            "name": "undo",
            "command": "git checkout . && git clean -xfd && npm i"
        },
        {
            "name": "push",
            "command": "gittey docs && gittey commit && git push"
        },
        {
            "name": "docs:form1",
            "command": "npm run build-form1-docs"
        },
        {
            "name": "docs:readme",
            "command": "npm run build-readme"
        },
        {
            "name": "docs",
            "command": "gittey docs:form1 && gittey docs:readme"
        }
    ],
    "collaborators": []
}