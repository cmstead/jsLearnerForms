{
    "branchPrefix": {
        "separator": "",
        "validator": "",
        "prefixes": {}
    },
    "commitPrefix": {
        "separator": " ",
        "validator": "^.{1,45}$",
        "prefixes": {
            "feat:": "Feature work",
            "refactor:": "Refactoring",
            "test:": "Test",
            "fix:": "Bug fix",
            "chore:": "Chore",
            "docs:": "Documentation updated/added",
            "style:": "Code style"
        }
    },
    "verboseMode": false,
    "aliases": [
        {
            "name": "undo",
            "command": "git checkout . && git clean -xfd && npm i"
        },
        {
            "name": "push",
            "command": "gittey docs && gittey commit && git push"
        },
        {
            "name": "docs:form1",
            "command": "npm run docs:form1"
        },
        {
            "name": "docs:form2",
            "command": "npm run docs:form2"
        },
        {
            "name": "docs:form3",
            "command": "npm run docs:form3"
        },
        {
            "name": "docs:readme",
            "command": "npm run docs:readme"
        },
        {
            "name": "docs:forms",
            "command": "npm run docs:forms"
        },
        {
            "name": "docs",
            "command": "gittey docs:form1 && gittey docs:form2 && gittey docs:form3 && gittey docs:forms && gittey docs:readme"
        }
    ],
    "collaborators": []
}